{
  "$id": "/fields/device.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Device",
  "description": "Dictionary of information about the device, containing id, manufacturer, model, name, type and version",
  "properties": {
    "id": {
      "type": "string"
    },
    "manufacturer": {
      "type": "string"
    },
    "model": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "version": {
      "type": "string"
    }
  },
  "type": "object"
}
